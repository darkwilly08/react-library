{
  "name": "darkwilly08",
  "version": "0.2.2",
  "license": "MIT",
  "scripts": {
    "start": "nx serve",
    "clean": "rm -rf ./dist && rm -f package-lock.json && rm -rf node_modules",
    "build": "nx build",
    "build:all": "nx run-many --target=build --all --exclude=shared-stories,workspace --parallel=4 --extractCss=false",
    "publish": "npm run build:all && nx run-many --target=publish --all --exclude=shared-stories,workspace --parallel=4",
    "test": "jest --config=global.jest.config.js --coverage",
    "test:nx": "nx test ${npm_config_name} -- --code-coverage --coverageReporters=text",
    "test:parallel": "nx run-many --target=test --coverageReporters='text' --all --exclude=shared-stories,workspace --parallel=4",
    "create-component": "nx g @nrwl/react:lib ${npm_config_name} --importPath=@darkwilly08/${npm_config_name}  --publishable",
    "storybook:start": "nx run shared-stories:storybook",
    "lint-scss": "stylelint **/*.scss",
    "lint-scss:fix": "npm run lint-scss -- --fix",
    "prepare": "husky install",
    "make-version": "nx run workspace:version --skipProjectChangelog --syncVersions",
    "make-version:patch": "nx run workspace:version --skipProjectChangelog --syncVersions --releaseAs=patch",
    "make-version:minor": "nx run workspace:version --skipProjectChangelog --syncVersions --releaseAs=minor",
    "make-version:major": "nx run workspace:version --skipProjectChangelog  --syncVersions --releaseAs=major"
  },
  "private": true,
  "dependencies": {
    "@types/testing-library__jest-dom": "^5.14.3",
    "core-js": "^3.6.5",
    "regenerator-runtime": "0.13.7",
    "tslib": "^2.3.0"
  },
  "devDependencies": {
    "@babel/core": "7.12.13",
    "@babel/preset-typescript": "7.12.13",
    "@commitlint/cli": "^16.2.4",
    "@commitlint/config-conventional": "^16.2.4",
    "@jscutlery/semver": "^2.23.4",
    "@nrwl/cli": "14.1.4",
    "@nrwl/cypress": "14.1.4",
    "@nrwl/eslint-plugin-nx": "14.1.4",
    "@nrwl/jest": "14.1.4",
    "@nrwl/linter": "14.1.4",
    "@nrwl/react": "14.1.4",
    "@nrwl/storybook": "^14.1.4",
    "@nrwl/web": "14.1.4",
    "@nrwl/workspace": "14.1.4",
    "@storybook/addon-essentials": "~6.4.12",
    "@storybook/builder-webpack5": "~6.4.12",
    "@storybook/core-server": "~6.4.12",
    "@storybook/manager-webpack5": "~6.4.12",
    "@storybook/react": "~6.4.12",
    "@svgr/webpack": "^5.4.0",
    "@testing-library/dom": "8.11.3",
    "@testing-library/jest-dom": "5.16.2",
    "@testing-library/react": "12.1.4",
    "@testing-library/react-hooks": "^8.0.0",
    "@types/jest": "27.4.1",
    "@types/node": "16.11.7",
    "@types/prop-types": "^15.7.5",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@typescript-eslint/eslint-plugin": "~5.18.0",
    "@typescript-eslint/parser": "~5.18.0",
    "babel-jest": "27.5.1",
    "babel-loader": "8.1.0",
    "cypress": "^9.1.0",
    "eslint": "~8.12.0",
    "eslint-config-prettier": "8.1.0",
    "eslint-plugin-cypress": "^2.10.3",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-jsx-a11y": "6.5.1",
    "eslint-plugin-react": "7.29.4",
    "eslint-plugin-react-hooks": "4.5.0",
    "husky": "^7.0.4",
    "identity-obj-proxy": "^3.0.0",
    "jest": "27.5.1",
    "nx": "14.1.4",
    "prettier": "^2.5.1",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-ripples": "^2.2.1",
    "react-test-renderer": "17.0.2",
    "stylelint": "^14.8.2",
    "stylelint-config-css-modules": "^4.1.0",
    "stylelint-config-standard-scss": "^3.0.0",
    "stylelint-scss": "^4.2.0",
    "ts-jest": "27.1.4",
    "ts-node": "9.1.1",
    "typescript": "~4.6.2",
    "url-loader": "^3.0.0"
  }
}
